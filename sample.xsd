                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
						
                        <xs:element name="entrevistador" type="xs:string"
							minOccurs="0">
                            <xs:annotation>
                                <xs:appinfo>
                                    <xhtml:label>4 Entrevistador</xhtml:label>
                                </xs:appinfo>
                            </xs:annotation>
                        </xs:element>
                        <xs:element name="titularBeneficiario" type="xs:string"
							minOccurs="0">
