                                    <xs:enumeration value="Taipa" />
                                    <xs:enumeration value="Madeira" />
                                    <xs:enumeration value="Pré-moldado" />
                                    <xs:enumeration value="Papelão" />
                                    <xs:enumeration value="Plástico" />
                                    <xs:enumeration value="Lona" />
                                    <xs:enumeration value="Mista" />
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
